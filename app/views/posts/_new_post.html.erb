<%= form_for(@new_post) do |f| %>
    <div class="panel panel-body panel-success content">
      <div class="row col-xs-12">
      <div class="col-xs-1">
        <div class="row">
          <img src="/user2.jpg" , class="img-circle" style="height: 35px;width: 35px;">
        </div>
      </div>
      <div class="col-xs-11">
        <div class="row">
          <%= f.text_field :content, class: "panel-background form-control", placeholder: "What's Happening?", style: "margin-left: 18px;"%>
        </div>
      </div>
    </div>
    </div>
<% end %>